<label>{config.label}</label>
<required>{config.required}</required>
<config>
	<type>input</type>
	<default>{config.default}</default>
	<size>{config.size}</size>
	<eval>{config.eval}</eval>
	<f:if condition="{config.range}">
		<range>
			<f:if condition="{config.range.lower}"><lower>{config.range.lower}</lower></f:if>
			<f:if condition="{config.range.upper}"><upper>{config.range.upper}</upper></f:if>
		</range>
	</f:if>
	<wizards type="array"><_PADDING type="integer">2</_PADDING>{config.wizards}</wizards>
</config>